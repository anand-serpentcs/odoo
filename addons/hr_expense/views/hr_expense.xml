<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend_expense" name="hr_expense assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/hr_expense/static/src/js/tour.js"></script>
                <script type="text/javascript" src="/hr_expense/static/src/js/web_planner_hr_expense.js"></script>
            </xpath>
        </template>
        <template id="hr_expense_template_refuse_sheet_reason">
            <p/>Your Expense  has been refused.</p><br/>
            <ul class="o_timeline_tracking_value_list">
                <li>Reason
                    <span> :
                    </span>
                    <span class="o_timeline_tracking_value">%s
                    </span>
                </li>
            </ul>
        </template>
    </data>
</odoo>
