<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Batch picking related subtypes for messaging / Chatter -->
    <record id="mt_batch_state" model="mail.message.subtype">
        <field name="name">Stage Changed</field>
        <field name="res_model">stock.batch.picking</field>
        <field name="default" eval="False"/>
        <field name="description">Stage Changed</field>
    </record>

    <record id="seq_batch_picking" model="ir.sequence">
        <field name="name">Batch Picking</field>
        <field name="code">batch.picking</field>
        <field name="prefix">Batch/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
