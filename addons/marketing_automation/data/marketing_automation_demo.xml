    <record id="server_action_message_sales_person" model="ir.actions.server">
        <field name="name">Message for sales person</field>
        <field name="model_id" ref="base.model_res_partner" />
        <field name="state">code</field>
        <field name="code">
            record.message_post(body='%s is interested for partner.' % record.name)
        </field>
    </record>

    <record id="activity_1" model="marketing.automation.activity">
        <field name="name">Offer free trial</field>
        <field name="mass_mailing_id" ref="mass_mailing_offer_free_trial" />
    </record>

    <record id="activity_2" model="marketing.automation.activity">
        <field name="name">After 7 days (if not partner yet)</field>
        <field name="mass_mailing_id" ref="mass_mailing_after_7_days" />
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="condition">[['category_id', 'not ilike', 'Partner']]</field>
    </record>

    <record id="activity_3" model="marketing.automation.activity">
        <field name="name">Message for sales person</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="trigger_type">mail_reply</field>
        <field name="activity_type">action</field>
        <field name="server_action_id" ref="server_action_message_sales_person" />
        <field name="parent_id" ref="activity_2" />
    </record>

